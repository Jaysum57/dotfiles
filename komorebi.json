{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.38/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.json",
  "window_hiding_behaviour": "Cloak",
  "window_handling_behaviour":"Sync",
  "cross_monitor_move_behaviour": "Insert",
  "resize_delta": 200,
  "mouse_follows_focus": false,
  "default_workspace_padding": 10,
  "default_container_padding": 4,
  "float_rule_placement":"None",
  "floating_layer_behaviour":"Float",
  "floating_layer_placement":"None",
  "floating_window_aspect_ratio": "Ultrawide",
  "border": true,
  "border_implementation":"Komorebi",
  "border_width": 4,
  "border_offset": -1,
  "transparency": false, 
  "transparency_alpha": 230,
  "transparency_ignore_rules": [
    {
      "id": "vlc.exe",
      "kind": "Exe",
      "matching_strategy": "Contains"
    }
  ],
  "border_colours": {
      "single": "#ffffff",
      "stack": "#ffffff",
      "monocle": "#da9125",
      "unfocused": "#3e3e3e00"
  },
  "animation": {
    "enabled": true,
    "duration": 40,
    "fps": 120,
    "style": {
      "movement": "EaseInOutCubic" 
    }
  },
  "stackbar": {
    "height": 30,
    "mode": "OnStack",
    "label": "Process",
    "tabs": {
      "width": 140,
      "font_size": 14,
      "font_family": "JetBrainsMono NFP",
      "background": "#0a0a0a",
      "focused_text": "#ffffff",
      "unfocused_text": "#888888"
    }
  },
  "monitors": [
    { 
      "window_based_work_area_offset": {
        "bottom": 100,
        "left": 150,
        "right": 300,
        "top": 20 
      },
      "workspaces": [
        {
          "name": "1",
          "layout": "BSP"
        },
        {
          "name": "2",
          "layout": "BSP" 
        },
        {
          "name": "3",
          "layout": "BSP"
        },
        {
          "name": "4",
          "layout": "BSP"
        }
      ]
    }
  ],
